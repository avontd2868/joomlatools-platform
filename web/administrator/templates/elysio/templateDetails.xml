<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install PUBLIC "-//Joomla! 2.5//DTD template 1.0//EN" "http://www.joomla.org/xml/dtd/2.5/template-install.dtd">
<extension version="1.0" type="template" client="administrator">
	<name>Elysio</name>
	<version>1.0</version>
	<creationDate>2017</creationDate>
	<author>Joomlatools</author>
	<copyright>Copyright (C) 2017 Timble CVBA (http://www.timble.net)</copyright>
	<description>Administrator template</description>
	<files>
		<folder>css</folder>
		<folder>fonts</folder>
		<folder>html</folder>
		<folder>images</folder>
		<folder>js</folder>
		<folder>language</folder>
		<folder>svg</folder>
		<filename>component.php</filename>
		<filename>cpanel.php</filename>
		<filename>error.php</filename>
		<filename>index.php</filename>
		<filename>login.php</filename>
		<filename>navigation.php</filename>
		<filename>template_preview.png</filename>
		<filename>template_thumbnail.png</filename>
		<filename>templateDetails.xml</filename>
	</files>
	<positions>
		<position>debug</position>
	</positions>
	<config>
		<fields name="params">
			<fieldset name="advanced">
				<field name="avatar" type="text" label="Avatar" description="Add URL to logo avatar file" />
				<field name="logo" type="text" label="Logo" description="Add URL to logo file" />
			</fieldset>
		</fields>
	</config>
</extension>